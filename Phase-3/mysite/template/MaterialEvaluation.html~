<script type="text/javascript">


function inference(percent)
{
   var str;
   
   if (percent <=25 )
   {
    str = "poor";
   } 
   else if ( percent <=50)
   {
    str = "average";
   }
   else if ( percent <= 75)
   {
    str = "good";
   }
   else  if( percent <= 100)
   {
    str = "excellent";
   }
   
   return str;
}

var inference_c1 = inference({% widthratio num_ques_correct_c1 num_ques_c1 100 %});
document.getElementById("inf_c1").innerHTML = inference_c1;
var inference_c2 = inference({% widthratio num_ques_correct_c2 num_ques_c2 100 %});
document.getElementById("inf_c2").innerHTML = inference_c2;
var inference_c3 = inference({% widthratio num_ques_correct_c3 num_ques_c3 100 %});
document.getElementById("inf_c3").innerHTML = inference_c3;
var inference_c4 = inference({% widthratio num_ques_correct_c4 num_ques_c4 100 %});
document.getElementById("inf_c4").innerHTML = inference_c4;

$(function () {
        $('#container').highcharts({
            chart: {
                type: 'column'
            },
            title: {
                text: 'Accuracy of Students in Concepts'
            },
            subtitle: {
                text: ''
            },
            xAxis: {
                type: 'category',
                labels: {
                    rotation: 0,
                    style: {
                        fontSize: '13px',
                        fontFamily: 'Verdana, sans-serif'
                    }
                }
            },
            yAxis: {
                min: 0,
                max: 100,
                title: {
                    text: 'Accuracy'
                }
            },
            legend: {
                enabled: false
            },
            tooltip: {
                pointFormat: 'Accuracy : <b>{point.y:.1f} % </b>',
            },
            series: [{
                name: 'Learning model Accuracies',
                data: [
                    ['Basics of C programming', {% widthratio num_ques_correct_c1 num_ques_c1 100 %}],
                    ['Decision Making', {% widthratio num_ques_correct_c2 num_ques_c2 100 %}],
                    ['Iterations', {% widthratio num_ques_correct_c3 num_ques_c3 100 %}],
                    ['Arrays and Structures', {% widthratio num_ques_correct_c4 num_ques_c4 100 %}],
                    
                                        
                ],
                dataLabels: {
                    enabled: true,
                    rotation: 0,
                    color: '#FFFFFF',
                    align: 'center',
                    x: 4,
                    y: 17,
                    style: {
                        fontSize: '13px',
                        fontFamily: 'Verdana, sans-serif',
                        textShadow: '0 0 3px black'
                    }
                }
            }]
        });
    });
    
