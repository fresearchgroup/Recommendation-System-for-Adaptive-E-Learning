$(function () {
        $('#container').highcharts({
            chart: {
                type: 'column'
            },
            title: {
                text: 'Learning Model Evaluation Results'
            },
            subtitle: {
                text: 'Average Ratings'
            },
            xAxis: {
                type: 'category',
                labels: {
                    rotation: 0,
                    style: {
                        fontSize: '13px',
                        fontFamily: 'Verdana, sans-serif'
                    }
                }
            },
            yAxis: {
                min: 0,
                max: 5,
                title: {
                    text: 'Rating'
                }
            },
            legend: {
                enabled: false
            },
            tooltip: {
                pointFormat: 'Average rating : <b>{point.y:.1f} out of 5</b>',
            },
            series: [{
                name: 'Learning model Characteristics',
                data: [
                    ['Effectiveness of Model', {% widthratio average1 3 1 %}],
                    ['Adaptivity of System', {% widthratio average2 3 1 %}],
                    ['State on Computer Programming', {% widthratio average3 3 1 %}],
                    ['Progress in Future', {% widthratio average4 3 1 %}],
                    ['Necessity of Revision', {% widthratio average5 3 1 %}],
                                        
                ],
                dataLabels: {
                    enabled: true,
                    rotation: 0,
                    color: '#FFFFFF',
                    align: 'center',
                    x: 4,
                    y: 15,
                    style: {
                        fontSize: '13px',
                        fontFamily: 'Verdana, sans-serif',
                        textShadow: '0 0 3px black'
                    }
                }
            }]
        });
    });
    


