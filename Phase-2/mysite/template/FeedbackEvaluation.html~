<script type="text/javascript">

function model_inference(a,b,c,d)
{
   var str;
   var n = (a+b+c)/(3*d);
   if (n < 1.5)
   {
    str = "not at all";
   } 
   else if ( n < 2.5)
   {
    str = "somewhat";
   }
   else if ( n < 3.5)
   {
    str = "to some extent yes";
   }
   else if ( n < 4.5)
   {
    str = "surely";
   }
   else
   {
    str = "very much"; 
   }
   return str;
}

function inference(a,b)
{
   var str;
   var n = a/b;
   if (n < 1.5)
   {
    str = "not at all";
   } 
   else if ( n < 2.5)
   {
    str = "somewhat";
   }
   else if ( n < 3.5)
   {
    str = "to some extent yes";
   }
   else if ( n < 4.5)
   {
    str = "surely";
   }
   else
   {
    str = "very much"; 
   }
   return str;
}
var inference1 = inference({{ans1.q1__sum}} ,{{num_stu.id}});
document.getElementById("inf1").innerHTML = inference1;
